{
  "name": "builder-react-comp",
  "version": "0.0.6",
  "description": "Builder Archetype - React Component",
  "main": "lib/index.js",
  "scripts": {
    "builder:compile": "rimraf lib && babel index.js --out-dir lib/ && babel scripts --out-dir lib/scripts && babel config --out-dir lib/config",
    "builder:gen-dev": "builder-support gen-dev",
    "prepublish": "npm run builder:compile",
    "eslint": "eslint --color src playground",
    "jscs": "jscs src playground",
    "lint": "builder concurrent eslint jscs",
    "test:all": "BABEL_ENV=commonjs jest --config node_modules/builder-react-comp/config/jest/config.json --colors",
    "test:all:watch": "BABEL_ENV=commonjs jest --config node_modules/builder-react-comp/config/jest/config.json --watch --colors",
    "test:all:coverage": "BABEL_ENV=commonjs jest --config node_modules/builder-react-comp/config/jest/config.coverage.json --colors",
    "serve:playground": "BABEL_ENV=commonjs node node_modules/builder-react-comp/lib/index serve playground",
    "build:commonjs": "rimraf lib && BABEL_ENV=commonjs babel ./src --out-dir ./lib --ignore **/__tests__/**",
    "build:es": "rimraf lib && BABEL_ENV=es babel ./src --out-dir ./es --ignore **/__tests__/**",
    "build": "builder concurrent build:commonjs build:es",
    "release": "node node_modules/builder-react-comp/lib/index release"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/olivierkastel/builder-react-comp-0.6.git"
  },
  "keywords": [
    "builder",
    "archetype",
    "react",
    "component"
  ],
  "author": "Olivier Kastel <olivier.kastel@gmail.com> (https://github.com/olivierkastel)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/olivierkastel/builder-react-comp-0.6/issues"
  },
  "homepage": "https://github.com/olivierkastel/builder-react-comp-0.6",
  "dependencies": {
    "babel-cli": "^6.5.0",
    "babel-core": "^6.5.0",
    "babel-loader": "^6.2.0",
    "babel-plugin-add-module-exports": "^0.1.1",
    "babel-plugin-check-es2015-constants": "^6.5.0",
    "babel-plugin-react-transform": "^2.0.0",
    "babel-plugin-transform-class-properties": "^6.5.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-es2015-arrow-functions": "^6.5.0",
    "babel-plugin-transform-es2015-block-scoped-functions": "^6.5.0",
    "babel-plugin-transform-es2015-block-scoping": "^6.5.0",
    "babel-plugin-transform-es2015-classes": "^6.5.0",
    "babel-plugin-transform-es2015-computed-properties": "^6.5.0",
    "babel-plugin-transform-es2015-destructuring": "^6.5.0",
    "babel-plugin-transform-es2015-duplicate-keys": "^6.5.0",
    "babel-plugin-transform-es2015-for-of": "^6.5.0",
    "babel-plugin-transform-es2015-function-name": "^6.5.0",
    "babel-plugin-transform-es2015-literals": "^6.5.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.5.0",
    "babel-plugin-transform-es2015-object-super": "^6.5.0",
    "babel-plugin-transform-es2015-parameters": "^6.5.0",
    "babel-plugin-transform-es2015-shorthand-properties": "^6.5.0",
    "babel-plugin-transform-es2015-spread": "^6.5.0",
    "babel-plugin-transform-es2015-sticky-regex": "^6.5.0",
    "babel-plugin-transform-es2015-template-literals": "^6.5.0",
    "babel-plugin-transform-es2015-typeof-symbol": "^6.5.0",
    "babel-plugin-transform-es2015-unicode-regex": "^6.5.0",
    "babel-plugin-transform-object-rest-spread": "^6.5.0",
    "babel-plugin-transform-regenerator": "^6.5.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "browser-sync": "^2.9.11",
    "file-loader": "^0.8.4",
    "json-loader": "^0.5.3",
    "raw-loader": "^0.5.1",
    "react": "^0.14.7",
    "react-addons-perf": "^0.14.7",
    "react-dom": "^0.14.7",
    "react-transform-hmr": "^1.0.2",
    "semver": "^5.1.0",
    "url-loader": "^0.5.6",
    "webpack": "^1.12.14",
    "webpack-dev-middleware": "^1.5.1",
    "webpack-hot-middleware": "^2.2.0"
  },
  "devDependencies": {
    "babel-eslint": "^5.0.0",
    "babel-preset-es2015": "^6.5.0",
    "builder": "^2.9.1",
    "builder-support": "^0.3.0",
    "eslint": "^2.2.0",
    "eslint-config-airbnb": "^6.1.0",
    "eslint-config-react": "^1.1.1",
    "eslint-plugin-react": "^4.1.0",
    "jscs": "^2.11.0"
  }
}
